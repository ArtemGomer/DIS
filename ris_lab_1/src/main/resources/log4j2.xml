<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="debug" name="lab1">
    <Appenders>
        <File name="FILE_LOG" fileName="logs/lab1.log" append="false">
            <PatternLayout>
                <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
            </PatternLayout>
        </File>
    </Appenders>
    <Loggers>
        <Root level="INFO">
            <AppenderRef ref="FILE_LOG"/>
        </Root>
    </Loggers>
</Configuration>